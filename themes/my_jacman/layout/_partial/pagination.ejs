<!-- 用在最后的通用型分页导航条， 文章列表的分页 -->
<nav class="pagenav clearfix">
	<% if (page.prev){ %>
		<a href="<%- config.root %><%- page.prev_link %>" title="<%= __('prev') %>" class="prevpage"><%= __('prev') %></a>
	<% } %>
	<% if (page.next){ %>
		<a href="<%- config.root %><%- page.next_link %>" title="<%= __('next') %>" class="nextpage"><%= __('next') %></a>
	<% } %>
</nav>